# Production Environment Configuration for Promptly Social

# Build Mode
NODE_ENV=production

# Disable Vite Debug
VITE_DEBUG=false

# Application Information
VITE_APP_NAME="Promptly Social"
VITE_APP_VERSION="0.1.0"

# API & Service Configuration
VITE_API_BASE_URL="https://api.promptlysocial.com"
VITE_USE_SECURE_STORAGE=true

# Analytics & Monitoring
VITE_ENABLE_TELEMETRY=true
VITE_ERROR_REPORTING=true

# Performance Settings
VITE_PREFETCH_RESOURCES=true
VITE_CACHE_STRATEGY="network-first" 